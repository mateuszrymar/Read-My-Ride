{
  "name": "read-my-ride",
  "version": "0.1.0",
  "description": "This web app lets users display statistics from their GPS tracking data.",
  "main": "dist/main.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "prebuild-dev": "shx rm -rf dist/*",
    "build-dev": "parcel build ./src/index.html --public-url ./",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build src/index.html",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"  
  },
  "dependencies": {
    "@zip.js/zip.js": "^2.6.61",
    "chartist": "^1.3.0",
    "leaflet": "^1.9.3"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "parcel-bundler": "^1.12.5",
    "shx": "^0.3.4"
  },
  "repository": {
